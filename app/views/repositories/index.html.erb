<div class = "container">
  <header><%= "Signed in as #{session[:username]}"%>
  <h1>Repos</h1>
    <%if @repos%>
      <ul>
        <%@repos.each do |repo|%>
          <li><%= repo['name'] %></li>
        <%end%>
      </ul>
    <%end%>

  <%= form_tag("/repositories/create", method: "post") do %>
    <%= label_tag(:name, "Create a New Repo:") %>
    <%= text_field_tag(:name, nil, id: "new-repo") %>
    <%= submit_tag("Create") %>
  <% end %>
</div>
