<div class = "container">

  <h1>Repos</h1>
  <ul>
  <% @results.each do |repo| %>
    <li><strong>><%= repo["name"] %></strong><br>
      <%= link_to(repo["html_url"],repo["html_url"]) %></li>
  <% end %>
  </ul>

  <%= form_tag("/repositories/create", method: "post") do %>
    <%= label_tag(:name, "Create a New Repo:") %>
    <%= text_field_tag(:name, nil, id: "new-repo") %>
    <%= submit_tag("Create") %>
  <% end %>
</div>



<%= @username %>
